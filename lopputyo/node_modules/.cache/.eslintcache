ame=\"btn\"\r\n        type=\"button\"\r\n        value=\"Fetch 1 quote\"\r\n      />\r\n      <div style={{\r\n        textAlign: \"center\",\r\n        fontWeight: 1.5,\r\n        color: \"#292b2c\"\r\n        }}>\r\n        <h3 style={{\r\n          fontSize: 40, \r\n          color: \"white\",\r\n          backgroundColor: \"#04AA6D\", \r\n          border: 100, \r\n          textAlign: \"center\",\r\n          borderRadius: \"22px\"\r\n          }}>\r\n          Word Count filter:\r\n          </h3>\r\n        <input \r\n        type=\"text\"\r\n        align=\"center\" \r\n        name=\"oneliner\" \r\n        id=\"sanat\" \r\n        />\r\n        <label className=\"font-light text-sm\" htmlFor=\"oneliner\"></label>\r\n      </div>\r\n      <div style={{textAlign: \"center\"}}>\r\n          {(() => {\r\n            if(quoteCount === 0){\r\n              return(<h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}}>No Quotes!</h2>)\r\n            }else{\r\n              return(<h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}}>Total quote count: {quoteCount}</h2>)\r\n            }\r\n          })()}\r\n        {/* <h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}}>Total quote count: {quoteCount}</h2> */}\r\n      </div>\r\n      <div>\r\n        {(() => {\r\n          if(quoteCount === 0){\r\n            return(<h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}} className=\"text-gray-500\">No Quotes!</h2>)\r\n          }else{\r\n            return(<h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}} className=\"text-gray-500\">Quotes in view: {quoteCount}</h2>)\r\n          }\r\n        })()}\r\n        {/* <h2 style={{fontSize: 40, color: \"green\", textAlign: \"center\"}} className=\"text-gray-500\">Quotes in view: {quoteCount}</h2> */}\r\n      </div>\r\n      <div>\r\n        <div style={{\r\n          textAlign: \"center\",\r\n          Color: \"red\"\r\n          }}>\r\n          <input\r\n            onClick={() => {\r\n              deleteKaikki();\r\n              kaikkiQuotet();\r\n            }}\r\n            type=\"button\"\r\n            value=\"Delete all quotes\"\r\n          />\r\n        </div>\r\n      </div>\r\n      <QuoteList deleteYksi={deleteYksi} />\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AddQuote;\r\n","C:\\Users\\jermu\\Desktop\\nettisivu\\ReactApp\\lopputyo\\src\\Components\\getQuotes.js",["41"],"import { useContext, useEffect } from \"react\"\r\nimport { AppContext } from \"../app-context\"\r\nconst GetQuotes = () => {\r\n    const [state, setState] = useContext(AppContext);\r\n    const loadQuote =  async () => {\r\n        const API_URL = \"https://api.kanye.rest/\"\r\n\r\n        let response = await fetch(API_URL)\r\n\r\n        let data = await response.json()\r\n\r\n        addQuote(data.quote)\r\n    }\r\n    const addQuote = (quote) => {\r\n        let new_quotes = [...state.quotes, quote]\r\n        setState({ ...state, quotes: new_quotes})\r\n    }\r\n\r\n    const deleteQuote = (quote) => {\r\n        let new_quotes = state.quotes.filter((element) => {\r\n            return !(\r\n                quote === element\r\n            )\r\n        })   \r\n        setState({...state, quotes: new_quotes})\r\n    }\r\n    const deleteQuotes = () => {\r\n        \r\n        setState({...state, quotes: []})\r\n    }\r\n\r\n    return{\r\n        loadQuote: loadQuote,\r\n        quoteAdd: addQuote,\r\n        deleteQuote: deleteQuote, \r\n        deleteQuotes: deleteQuotes,\r\n        quotes: state.quotes,\r\n        quote: state.quote,\r\n    }\r\n}\r\nexport default GetQuotes;","C:\\Users\\jermu\\Desktop\\nettisivu\\ReactApp\\lopputyo\\src\\Components\\QuoteList.js",[],"C:\\Users\\jermu\\Desktop\\nettisivu\\ReactApp\\lopputyo\\src\\Components\\Quote.js",[],{"ruleId":"42","severity":1,"message":"43","line":1,"column":17,"nodeType":"44","messageId":"45","endLine":1,"endColumn":26},{"ruleId":"42","severity":